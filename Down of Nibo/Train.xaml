<Page x:Class="Down_of_Nibo.Train"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:gif="http://wpfanimatedgif.codeplex.com" 
      xmlns:local="clr-namespace:Down_of_Nibo"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="Train">

    <Grid>
        <ProgressBar Height="40" VerticalAlignment="Top" Grid.Column="1" Name="ActionTime" />
        <Label Height="50" Margin="0 40 0 0" VerticalAlignment="Top" Grid.Column="1"  FontSize="35"  Name="ComboPress" ></Label>
        <Label Height="50"  VerticalAlignment="Center" Grid.Column="1"  FontSize="35"  Name="ComboToCopyVisual" ></Label>
        <Label Name="TestSpace"></Label>
        <Image gif:ImageBehavior.AnimatedSource="Assets\sprites\main\Attack.gif" RenderOptions.BitmapScalingMode="NearestNeighbor" Height="150" HorizontalAlignment="Center" VerticalAlignment="Bottom"  />
    </Grid>
</Page>
